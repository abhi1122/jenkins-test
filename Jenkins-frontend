pipeline {
  agent any
  tools {nodejs "nodejs"}  
  stages {

    stage('Build') {
      steps {
        dir("frontend") {
            sh "pwd"
            sh 'yarn'
        }  
      }
    }
    
    stage('Start') {
      steps {
        dir("frontend") {
            sh "pwd"
            sh 'yarn start dev'
        }  
      }
    }

  }
}
